{
  "name": "@zettabrasil/react-native-searchbar",
  "version": "1.0.11",
  "description": "Search Bar for iOS & Android with voice and history features",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "release": "standard-version",
    "postinstall": "husky install"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/zettabrasil/react-native-searchbar.git"
  },
  "keywords": [
    "search",
    "bar",
    "react",
    "native"
  ],
  "author": "Darlei Kroth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zettabrasil/react-native-searchbar/issues"
  },
  "homepage": "https://github.com/zettabrasil/react-native-searchbar#readme",
  "dependencies": {
    "color": "^3.1.3",
    "react-native-platform-touchable": "^1.1.1",
    "react-native-root-siblings": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^6.0.0",
    "standard-version": "^9.3.0"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": "^1.15.0",
    "@react-native-voice/voice": "^3.1.3",
    "color": "^3.1.3",
    "react-native-platform-touchable": "^1.1.1",
    "react-native-root-siblings": "^4.1.0"
  }
}
